{
  "builder": "qmake",
  "framework": "ubuntu-touch-24.04-1.x",
  "default": "clean build click-build",
  "kill": "harbour-owncloud",
  "dependencies_target": [
    "liblomiri-download-manager-client-dev",
    "liblomiri-online-accounts-qt5-dev",
    "libgles-dev"
  ],
  "build_args": [
    "CONFIG+=nodaemon",
    "CONFIG+=nopermissionagent",
    "CONFIG+=noqmlextension",
    "CONFIG+=nosharing",
    "CONFIG+=quickcontrols",
    "CONFIG+=ubuntutouch",
    "CONFIG+=click"
  ],
  "make_jobs": 8,
  "qt_version": "5.15"
}
