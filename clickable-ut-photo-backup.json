{
  "builder": "qmake",
  "default": "clean build click-build",
  "kill": "harbour-owncloud-photobackup",
  "dependencies_target": [
    "liblomiri-download-manager-client-dev",
    "liblomiri-online-accounts-qt5-dev"
  ],
  "build_args": [
    "CONFIG+=noapp",
    "CONFIG+=nopermissionagent",
    "CONFIG+=noqmlextension",
    "CONFIG+=nosharing",
    "CONFIG+=clickphotobackup"
  ],
  "make_jobs": 8,
  "qt_version": "5.15",
  "framework": "ubuntu-touch-24.04-1.x"
}
